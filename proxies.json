{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "permanent redirect": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/"
            },
            "responseOverrides": {
                "response.statusCode": "301",
                "response.headers.Location": "/api/runapp"
            }
        }
    }
}